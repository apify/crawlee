        "prop-types": "^15.8.1",
        "raw-loader": "^4.0.2",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "react-github-btn": "^1.4.0",
        "react-lite-youtube-embed": "^2.3.52",
