        "@apify/docusaurus-plugin-typedoc-api": "^4.4.8",
        "@apify/utilities": "^2.8.0",
        "@docusaurus/core": "3.9.1",
        "@docusaurus/faster": "3.9.1",
        "@docusaurus/mdx-loader": "3.9.1",
        "@docusaurus/plugin-client-redirects": "3.9.1",
